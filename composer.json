{
  "name": "stryber/laravel-metrics",
  "description": "Stryber Laravel metrics collector",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Naidenko Dmytro",
      "email": "dmitryi@stryber.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "illuminate/contracts": "^8.0",
    "illuminate/support": "^8.0",
    "illuminate/database": "^8.0",
    "illuminate/http": "^8.0",
    "influxdata/influxdb-client-php": "^1.10",
    "ramsey/uuid": "^4.1",
    "stryber/laravel-uuid-helper": "^8.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Stryber\\Metrics\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Stryber\\Metrics\\MetricsServiceProvider"
      ]
    }
  }
}
